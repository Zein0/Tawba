{
  "expo": {
    "name": "Tawba",
    "slug": "tawba",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "tawba",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f2ede4"
    },
    "assetBundlePatterns": [],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.tawba.app",
      "icon": "./assets/icon.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.tawba.app",
      "versionCode": 1,
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f2ede4"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      [
        "expo-notifications"
      ],
      [
        "expo-localization"
      ],
      [
        "expo-location"
      ],
      [
        "expo-router"
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "74fa6883-6da0-4d6a-944f-95e623dd22dc"
      }
    },
    "owner": "zein00"
  }
}
